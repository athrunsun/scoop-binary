            "32bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/AdoptOpenJDK/11/jdk/x32/windows/OpenJDK$matchJdkU-jdk_x86-32_windows_hotspot_$matchMajor_$matchBuild.zip"
            }
        },
        "extract_dir": "jdk-$matchMajor+$matchBuild"
    }
}
