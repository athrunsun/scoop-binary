{
    "version": "2.30.2",
    "homepage": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git",
    "description": "Distributed version control system. Will download from China TUNA mirror.",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.30.2/PortableGit-2.30.2-64-bit.7z.exe#/dl.7z",
            "hash": "f719f248de3dd7ef234331f8da95762594a388f6aa62f4c0260df18068e5a447"
        },
        "32bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%202.30.2/PortableGit-2.30.2-32-bit.7z.exe#/dl.7z",
            "hash": "8b203531c91d3f9075aa3ef1e89b0d6e5d18aa289c3bc485e093c9bfb860a116"
        }
    },
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "git-bash.exe",
        "usr\\bin\\ssh.exe",
        "usr\\bin\\sshd.exe",
        "usr\\bin\\scp.exe",
        "usr\\bin\\sftp.exe",
        "usr\\bin\\ssh-add.exe",
        "usr\\bin\\ssh-agent.exe",
        "usr\\bin\\ssh-keygen.exe",
        "usr\\bin\\ssh-keyscan.exe",
        "usr\\bin\\tig.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "post_install": "git config --global credential.helper manager",
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v[\\w.]+/PortableGit-([\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/Git%20for%20Windows%20$version/PortableGit-$version-32-bit.7z.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "https://github.com/git-for-windows/git/releases/latest",
            "regex": "<td>$basename</td>\\s*<td>$sha256</td>"
        }
    }
}
